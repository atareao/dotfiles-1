set $border #e6b450
set $foreground #6c7680
set $foreground_inactive #bfbab0
set $transparent #00000000

# class
#   border
#   background
#   text
#   indicator
#   child_border

client.focused \
  $border \
  $border \
  $foreground \
  $border \
  $border

client.unfocused \
  $transparent \
  $transparent \
  $foreground_inactive \
  $transparent \
  $transparent
  
client.focused_inactive \
  $transparent \
  $transparent \
  $foreground_inactive \
  $transparent \
  $transparent